      :key=\"term.value\"\r\n                :label=\"term.label\"\r\n                :value=\"term.value\"\r\n              >\r\n              </el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n          <div style=\"margin-left: 45%\">\r\n            <span slot=\"footer\">\r\n              <el-button @click=\"newClassDialogVisible = false\" size=\"mini\"\r\n                >取消</el-button\r\n              >\r\n              <el-button type=\"primary\" size=\"mini\" @click=\"addClass\"\r\n                >确认添加</el-button\r\n              >\r\n            </span>\r\n          </div>\r\n        </el-form>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      pwdDialogFlag: false,\r\n      username: this.$store.getters.getUser.role.username,\r\n      updateGroupData: {\r\n        uid: this.$store.getters.getUser.role.uid,\r\n        username: this.$store.getters.getUser.role.username,\r\n        password: this.$store.getters.getUser.role.password,\r\n        role: this.$store.getters.getUser.role.role,\r\n        groupId: this.$store.getters.getUser.role.groupId,\r\n      },\r\n      // user: JSON.parse(window.sessionStorage.getItem('user')),\r\n    };\r\n  },\r\n  // mounted(){\r\n  //   console.log(this.$store.getters.getUser.role.uid)\r\n  // },\r\n  methods: {\r\n    // menuClick(index) {\r\n    //   this.$router.push(index);\r\n    // },\r\n    handleCommand(command) {\r\n      if (command == \"logout\") {\r\n        sessionStorage.clear();\r\n        this.$router.replace(\"/\");\r\n        this.$message({\r\n          message: \"注销成功\",\r\n          type: \"success\",\r\n        });\r\n      } else if (command == \"changePwd\") this.pwdDialogFlag = true;\r\n    },\r\n    updatePwd() {\r\n      this.$confirm(\"确定提交吗?\", \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\",\r\n      })\r\n        .then((resp) => {\r\n          this.putRequest(\"/user/updateUser\", this.updateGroupData).then(\r\n            (resp) => {\r\n              if (resp.data.msg === \"成功\") {\r\n                this.loadGroup();\r\n                this.$message({\r\n                  message: \"修改成功\",\r\n                  type: \"success\",\r\n                });\r\n                this.updateGroupDialogVisible = false;\r\n              } else {\r\n                this.$message({\r\n                  message: \"修改失败\",\r\n                  type: \"error\",\r\n                });\r\n              }\r\n            }\r\n          );\r\n        })\r\n        .catch(() => {\r\n          this.$message({\r\n            type: \"info\",\r\n            message: \"已取消提交\",\r\n          });\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.homeHeader {\r\n  background-color: LightSkyBlue;\r\n  opacity: 0.8;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  padding: 0 15px;\r\n}\r\n.homeHeader .title {\r\n  font-size: 30px;\r\n}\r\n.homeHeader .userinfo {\r\n  cursor: pointer;\r\n}\r\n.el-dropdown-link {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n.el-dropdown-link img {\r\n  width: 48px;\r\n  height: 48px;\r\n  border-radius: 24px;\r\n  margin-left: 8px;\r\n}\r\n</style>"],"mappings":"AAkEA;EACAA,KAAA;IACA;MACAC,aAAA;MACAC,QAAA,OAAAC,MAAA,CAAAC,OAAA,CAAAC,OAAA,CAAAC,IAAA,CAAAJ,QAAA;MACAK,eAAA;QACAC,GAAA,OAAAL,MAAA,CAAAC,OAAA,CAAAC,OAAA,CAAAC,IAAA,CAAAE,GAAA;QACAN,QAAA,OAAAC,MAAA,CAAAC,OAAA,CAAAC,OAAA,CAAAC,IAAA,CAAAJ,QAAA;QACAO,QAAA,OAAAN,MAAA,CAAAC,OAAA,CAAAC,OAAA,CAAAC,IAAA,CAAAG,QAAA;QACAH,IAAA,OAAAH,MAAA,CAAAC,OAAA,CAAAC,OAAA,CAAAC,IAAA,CAAAA,IAAA;QACAI,OAAA,OAAAP,MAAA,CAAAC,OAAA,CAAAC,OAAA,CAAAC,IAAA,CAAAI;MACA;MACA;IACA;EACA;;EACA;EACA;EACA;EACAC,OAAA;IACA;IACA;IACA;IACAC,cAAAC,OAAA;MACA,IAAAA,OAAA;QACAC,cAAA,CAAAC,KAAA;QACA,KAAAC,OAAA,CAAAC,OAAA;QACA,KAAAC,QAAA;UACAC,OAAA;UACAC,IAAA;QACA;MACA,WAAAP,OAAA,sBAA