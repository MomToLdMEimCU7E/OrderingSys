r\n  },\r\n  methods: {\r\n    AdvmgtDialogClose() {\r\n      this.$emit(\"update:AdvmgtDialogFlag\", false);\r\n      //   console.log(this.OrderinfoDialogFlag)\r\n    },\r\n    loadStatus(){\r\n      this.getRequest(\r\n        \"/user/advertiseStatus?meetingId=\" +\r\n          this.CmeetingId \r\n      ).then((resp) => {\r\n        if (resp) {\r\n          this.groupData = resp.data.data;\r\n        }\r\n      });\r\n    },\r\n    endAdv(){\r\n      this.postRequest(\r\n            \"/order/generateSequence?meetingId=\" +\r\n              this.loginForm.username +\r\n              \"&password=\" +\r\n              this.loginForm.password\r\n          ).then((resp) => {\r\n            if (resp.data.code == \"0\") {\r\n              //存储用户token\r\n              // const tokenStr = resp.obj.tokenHead + resp.obj.token;\r\n              // window.sessionStorage.setItem(\"tokenStr\", tokenStr);\r\n              this.$store.dispatch('asyncUpdateUser',{role:resp.data.data})\r\n              //跳转页面\r\n              this.$router.replace(\"/home\");\r\n              this.$message({\r\n                message: \"登录成功\",\r\n                type: \"success\",\r\n              });\r\n            } else {\r\n              this.loading = false;\r\n              this.$message.error(\"登陆失败，请检查账号密码\");\r\n            }\r\n          });\r\n      this.$emit(\"update:AdvmgtDialogFlag\", false);\r\n    },\r\n  },\r\n  watch: {\r\n    AdvmgtDialogFlag() {\r\n      this.visible = this.AdvmgtDialogFlag;\r\n      this.CmeetingId = this.meetingId;\r\n      this.loadStatus();\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n</style>"],"mappings":"AAmCA;EACAA,IAAA;EACAC,KAAA;IACAC,gBAAA;MACAC,OAAA;IACA;IACAC,SAAA;MACAC,IAAA,EAAAC;IACA;EACA;EACAC,KAAA;IACA;MACAC,OAAA;MACAC,UAAA;MACAC,SAAA,GACA;QACAV,IAAA;QACAW,IAAA;MACA,GACA;QACAX,IAAA;QACAW,IAAA;MACA,GACA;QACAX,IAAA;QACAW,IAAA;MACA;IAEA;EACA;EACAC,OAAA;IACAC,kBAAA;MACA,KAAAC,KAAA;MACA;IACA;;IACAC,WAAA;MACA,KAAAC,UAAA,CACA,qCACA,KAAAP