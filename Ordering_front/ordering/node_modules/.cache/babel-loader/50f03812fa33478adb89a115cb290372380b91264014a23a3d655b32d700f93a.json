      >否</el-tag\r\n              >\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n      </div>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"AdvmgtDialogClose\">退出</el-button>\r\n        <el-button type=\"primary\" @click=\"AdvmgtDialogClose\"\r\n          >结束广告投放</el-button\r\n        >\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"AdvmgtDialog\",\r\n  props: {\r\n    AdvmgtDialogFlag: {\r\n      default: false,\r\n    },\r\n    meetingId: {\r\n      type: Number,\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      visible: false,\r\n      CmeetingId: '',\r\n      groupData: [\r\n        {\r\n          name: \"米奇妙妙屋\",\r\n          flag: \"1\",\r\n        },\r\n        {\r\n          name: \"赚他一个亿\",\r\n          flag: \"1\",\r\n        },\r\n        {\r\n          name: \"卓子公司\",\r\n          flag: \"0\",\r\n        },\r\n      ],\r\n    };\r\n  },\r\n  methods: {\r\n    AdvmgtDialogClose() {\r\n      this.$emit(\"update:AdvmgtDialogFlag\", false);\r\n      //   console.log(this.OrderinfoDialogFlag)\r\n    },\r\n    loadStatus(){\r\n      this.getRequest(\r\n        \"/user/historyQuery?meetingId=\" +\r\n          this.CmeetingId \r\n      ).then((resp) => {\r\n        if (resp) {\r\n          this.groupData = resp.data.data;\r\n        }\r\n      });\r\n    },\r\n  },\r\n  watch: {\r\n    AdvmgtDialogFlag() {\r\n      this.visible = this.AdvmgtDialogFlag;\r\n      this.CmeetingId = this.meetingId;\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n</style>"],"mappings":"AAmCA;EACAA,IAAA;EACAC,KAAA;IACAC,gBAAA;MACAC,OAAA;IACA;IACAC,SAAA;MACAC,IAAA,EAAAC;IACA;EACA;EACAC,KAAA;IACA;MACAC,OAAA;MACAC,UAAA;MACAC,SAAA,GACA;QACAV,IAAA;QACAW,IAAA;MACA,GACA;QACAX,IAAA;QACAW,IAAA;MACA,GACA;QACAX,IAAA;QACAW,IAAA;MACA;IAEA;EACA;EACAC,OAAA;IACAC,kBAAA;MACA,KAAAC,KAAA;MACA;IACA;;IACAC,WAAA;MACA,KAAAC,UAAA,CACA,kCACA,KAAAP,