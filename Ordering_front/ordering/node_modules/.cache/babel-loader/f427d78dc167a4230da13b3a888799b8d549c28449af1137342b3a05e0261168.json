em>\r\n          <div style=\"margin-left: 45%\">\r\n            <span slot=\"footer\">\r\n              <el-button @click=\"newClassDialogVisible = false\" size=\"mini\"\r\n                >取消</el-button\r\n              >\r\n              <el-button type=\"primary\" size=\"mini\" @click=\"addClass\"\r\n                >确认添加</el-button\r\n              >\r\n            </span>\r\n          </div>\r\n\r\n          <!-- <span slot=\"footer\">\r\n            \r\n          </span> -->\r\n        </el-form>\r\n      </div>\r\n    </el-dialog>\r\n    <div class=\"bg_div\">\r\n      <el-table\r\n        :data=\"classData\"\r\n        v-loading=\"loading\"\r\n        style=\"width: 100%\"\r\n        :header-cell-style=\"tableHeaderColor\"\r\n      >\r\n        <el-table-column prop=\"week\" label=\"班级\"> </el-table-column>\r\n        <el-table-column prop=\"yearOfTerm\" label=\"学年\"> </el-table-column>\r\n        <el-table-column prop=\"term\" label=\"学期\"> </el-table-column>\r\n        <el-table-column prop=\"year\" label=\"当前订货年度\" width=\"120\">\r\n        </el-table-column>\r\n        <el-table-column label=\"操作\" width=\"300\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button type=\"text\" @click=\"openClassDialog(scope.row)\">\r\n              查看小组\r\n            </el-button>\r\n            <el-button type=\"text\"> 更改 </el-button>\r\n            <el-button type=\"text\"> 删除班级 </el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </div>\r\n    <el-dialog title=\"提示\" :visible.sync=\"ClassDialogFlag\" width=\"30%\">\r\n      <el-table :data=\"groupData\" style=\"width: 100%\">\r\n        <el-table-column prop=\"username\" label=\"小组名称\">\r\n             <template slot-scope=\"scope\">\r\n          <span>{{ scope.row.role == \"\"? \"仅退款\": scope.row.type == 2 ? \"仅换货\" : \"退货退款\"}}</span>\r\n        </template>\r\n      </el-table>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-butt